<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="breeze.js">
            <arguments>
                <argument name="bundles" xsi:type="array">
                    <item name="default" xsi:type="array">
                        <item name="items" xsi:type="array">
                            <item name="Swissup_Testimonials/js/list" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_Testimonials/js/list</item>
                                <item name="load" xsi:type="array">
                                    <item name="onReveal" xsi:type="array">
                                        <item name="0" xsi:type="string">[data-mage-init*="Swissup_Testimonials/js/list"]</item>
                                    </item>
                                </item>
                            </item>
                            <item name="Swissup_Testimonials/js/side-list-widget" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_Testimonials/js/side-list-widget</item>
                                <item name="load" xsi:type="array">
                                    <item name="onReveal" xsi:type="array">
                                        <item name="0" xsi:type="string">[data-mage-init*="Swissup_Testimonials/js/side-list-widget"]</item>
                                    </item>
                                </item>
                            </item>
                        </item>
                    </item>
                </argument>
            </arguments>
        </referenceBlock>
    </body>
</page>
